<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">


    <ScrollView
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="0dp"
        android:fillViewport="true"

        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/tv_kqypg_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:text="样品罐"
                android:textColor="@color/black"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/tv_kqypg_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="5dp"
                android:text="检测次数："
                android:textColor="@color/black"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <TextView
                android:id="@+id/view1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/dividing_line"
                android:layerType="software"
                app:layout_constraintTop_toBottomOf="@+id/tv_kqypg_name" />


            <TextView
                android:id="@+id/tv_kqyj_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:text="乙腈"
                android:textColor="@color/black"
                app:layout_constraintLeft_toLeftOf="@id/tv_kqypg_name"
                app:layout_constraintTop_toBottomOf="@id/view1" />

            <TextView
                android:id="@+id/tv_kqyj_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:text="检测次数："
                android:textColor="@color/black"
                app:layout_constraintRight_toRightOf="@id/tv_kqypg_num"
                app:layout_constraintTop_toBottomOf="@id/view1" />

            <TextView
                android:id="@+id/view2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/dividing_line"
                android:layerType="software"
                app:layout_constraintTop_toBottomOf="@+id/tv_kqyj_num" />


            <TextView
                android:id="@+id/tv_kqspe_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:text="SPE柱"
                android:textColor="@color/black"
                app:layout_constraintLeft_toLeftOf="@id/tv_kqypg_name"
                app:layout_constraintTop_toBottomOf="@id/view2" />

            <TextView
                android:id="@+id/tv_kqspe_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:text="检测次数："
                android:textColor="@color/black"
                app:layout_constraintRight_toRightOf="@id/tv_kqypg_num"
                app:layout_constraintTop_toBottomOf="@id/view2" />

            <TextView
                android:id="@+id/view3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/dividing_line"
                android:layerType="software"
                app:layout_constraintTop_toBottomOf="@+id/tv_kqspe_num" />


            <TextView
                android:id="@+id/tv_kqxt_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:text="吸头"
                android:textColor="@color/black"
                app:layout_constraintLeft_toLeftOf="@id/tv_kqypg_name"
                app:layout_constraintTop_toBottomOf="@id/view3" />

            <TextView
                android:id="@+id/tv_kqxt_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:text="检测次数："
                android:textColor="@color/black"
                app:layout_constraintRight_toRightOf="@id/tv_kqypg_num"
                app:layout_constraintTop_toBottomOf="@id/view3" />

            <TextView
                android:id="@+id/view4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/dividing_line"
                android:layerType="software"
                app:layout_constraintTop_toBottomOf="@+id/tv_kqxt_num" />


            <TextView
                android:id="@+id/tv_kqnsg_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:text="浓缩管"
                android:textColor="@color/black"
                app:layout_constraintLeft_toLeftOf="@id/tv_kqypg_name"
                app:layout_constraintTop_toBottomOf="@id/view4" />

            <TextView
                android:id="@+id/tv_kqnsg_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="15dp"
                android:layout_marginBottom="5dp"
                android:text="检测次数："
                android:textColor="@color/black"
                app:layout_constraintRight_toRightOf="@id/tv_kqypg_num"
                app:layout_constraintTop_toBottomOf="@id/view4" />

            <TextView
                android:id="@+id/view5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/dividing_line"
                android:layerType="software"
                app:layout_constraintTop_toBottomOf="@+id/tv_kqnsg_num" />


        </androidx.constraintlayout.widget.ConstraintLayout>

    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>
