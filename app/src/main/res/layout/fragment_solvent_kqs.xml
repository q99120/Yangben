        android:textColor="@color/black"
        app:layout_constraintLeft_toLeftOf="@id/view"
        app:layout_constraintTop_toBottomOf="@+id/tv_kq_hhsj" />

    <TextView
        android:id="@+id/tv_title_syzy"
